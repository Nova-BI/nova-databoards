{
  "name": "nova-bi/nova-databoards",
  "description": "Powerful BI Databoards for Nova",
  "keywords": [
    "laravel",
    "nova",
    "databoard",
    "dashboard",
    "analytics"
  ],
  "license": "MIT",
  "support": {
    "email": "github@mailbeez.com",
    "issues": "https://github.com/Nova-BI/nova-databoards/issues",
    "source": "https://github.com/Nova-BI/nova-databoards",
    "docs": "https://github.com/Nova-BI/nova-databoards/issues/blob/master/README.md"
  },
  "authors": [
    {
      "name": "Cord F. Rosted",
      "homepage": "https://www.linkedin.com/in/cordfocke/",
      "email": "github@mailbeez.com",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ampeco/nova-date-range-filter": "^0.1.1",
    "comodolab/nova-help": "^0.3",
    "digital-creative/collapsible-resource-manager": "^1.2",
    "digital-creative/nova-inline-morph-to": "^0.2",
    "digitalazgroup/nova-plain-text-field": "^0.0.4",
    "dillingham/nova-attach-many": "^1.0",
    "dillingham/nova-detail-link": "^1.0",
    "eminiarts/nova-relationship-selector": "^1.0",
    "eminiarts/nova-tabs": "^1.0",
    "epartment/nova-dependency-container": "^1.2.10",
    "ericlagarda/nova-text-card": "^1.1",
    "klepak/nova-multiselect-filter": "^1.2",
    "nemrutco/nova-global-filter": "^1.0",
    "optimistdigital/nova-sortable": "^1.0",
    "pdmfc/nova-info-card": "^1.0",
    "rcknr/nova-multiselect-filter": "^1.3",
    "spatie/laravel-schemaless-attributes": "^1.5",
    "saumini/count": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "NovaBI\\NovaDataboards\\": "src/",
      "NovaBI\\NovaDataboards\\Nova\\": "src/Nova/",
      "NovaBI\\NovaDataboards\\Nova\\Databoardables\\": "src/Nova/Databoardables"
    },
    "files": [
      "src/Helpers/Files.php",
      "src/Nova/Databoardables/Standard.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "NovaBI\\NovaDataboards\\NovaDataboardsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
